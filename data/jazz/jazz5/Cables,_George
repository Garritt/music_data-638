ared Secrets","byArtist":{"@id":"https://musicbrainz.org/artist/7d3aa1dd-5e8b-47f3-a65b-41d78c725ce3","name":"George Cables","@type":["Person","MusicGroup"]},"creditedTo":"George Cables","@type":"MusicAlbum"},{"@type":"MusicAlbum","creditedTo":"George Cables","byArtist":{"@id":"https://musicbrainz.org/artist/7d3aa1dd-5e8b-47f3-a65b-41d78c725ce3","name":"George Cables","@type":["Person","MusicGroup"]},"name":"Looking for the Light","@id":"https://musicbrainz.org/release-group/e80f0769-e14b-4b6e-a67e-fc6231e61771","albumProductionType":"http://schema.org/StudioAlbum","albumReleaseType":"http://schema.org/AlbumRelease"},{"albumProductionType":"http://schema.org/StudioAlbum","albumReleaseType":"http://schema.org/AlbumRelease","@id":"https://musicbrainz.org/release-group/f526a8c2-bd3c-37ab-a175-4a6b25926563","name":"Portal","byArtist":[{"@type":"MusicGroup","@id":"https://musicbrainz.org/artist/17d164a8-c5c8-4ab9-94b7-0ea2a4cfd0e5","name":"The Deardorf/Peterson Group"},{"name":"George Cables","@id":"https://musicbrainz.org/artist/7d3aa1dd-5e8b-47f3-a65b-41d78c725ce3","@type":["Person","MusicGroup"]}],"creditedTo":"The Deardorf/Peterson Group feat. George Cables","@type":"MusicAlbum"},{"byArtist":[{"@type":["Person","MusicGroup"],"name":"Art Pepper","@id":"https://musicbrainz.org/artist/266b9126-4a40-4b9b-b21e-422d72e64254"},{"name":"George Cables","@id":"https://musicbrainz.org/artist/7d3aa1dd-5e8b-47f3-a65b-41d78c725ce3","@type":["Person","MusicGroup"]}],"albumReleaseType":"http://schema.org/AlbumRelease","albumProductionType":"http://schema.org/StudioAlbum","@id":"https://musicbrainz.org/release-group/ed037ab4-2c0b-375d-8ad0-5ac561b85872","name":"Goin' Home","@type":"MusicAlbum","creditedTo":"Art Pepper and George Cables"},{"creditedTo":"George Cables","@type":"MusicAlbum","name":"Why Not?","@id":"https://musicbrainz.org/release-group/2b2d8d68-cf25-4e67-bee8-3251985c8cf0","albumProductionType":"http://schema.org/StudioAlbum","albumReleaseType":"http://schema.org/AlbumR