a1"},{"@id":"https://musicbrainz.org/release-group/d6de6c72-9374-3087-b1c1-8a6419286696","albumProductionType":"http://schema.org/CompilationAlbum","byArtist":{"@id":"https://musicbrainz.org/artist/c5a0a216-c33c-42dc-92b8-4b60236f8dad","name":"Andrea Bocelli","@type":["Person","MusicGroup"]},"creditedTo":"Andrea Bocelli","name":"Greatest Hits","albumReleaseType":"http://schema.org/AlbumRelease","@type":"MusicAlbum"},{"@id":"https://musicbrainz.org/release-group/09acb450-3b26-3ef1-9d47-f106466f39ac","albumProductionType":"http://schema.org/LiveAlbum","@type":"MusicAlbum","albumReleaseType":"http://schema.org/AlbumRelease","name":"Under the Desert Sky","creditedTo":"Andrea Bocelli","byArtist":{"@id":"https://musicbrainz.org/artist/c5a0a216-c33c-42dc-92b8-4b60236f8dad","@type":["Person","MusicGroup"],"name":"Andrea Bocelli"}},{"name":"Vivere: Live in Tuscany","creditedTo":"Andrea Bocelli","byArtist":{"name":"Andrea Bocelli","@type":["Person","MusicGroup"],"@id":"https://musicbrainz.org/artist/c5a0a216-c33c-42dc-92b8-4b60236f8dad"},"albumReleaseType":"http://schema.org/AlbumRelease","@type":"MusicAlbum","albumProductionType":"http://schema.org/LiveAlbum","@id":"https://musicbrainz.org/release-group/5465036a-1cf9-3a1b-a077-bb130e2bf44d"},{"albumProductionType":"http://schema.org/LiveAlbum","@id":"https://musicbrainz.org/release-group/7453434f-804c-43db-a679-0b88920d9e1d","byArtist":{"@id":"https://musicbrainz.org/artist/c5a0a216-c33c-42dc-92b8-4b60236f8dad","@type":["Person","MusicGroup"],"name":"Andrea Bocelli"},"name":"Concerto: One Night in Central Park","creditedTo":"Andrea Bocelli","@type":"MusicAlbum","albumReleaseType":"http://schema.org/AlbumRelease"},{"@type":"MusicAlbum","albumReleaseType":"http://schema.org/SingleRelease","byArtist":{"name":"Andrea Bocelli","@type":["Person","MusicGroup"],"@id":"https://musicbrainz.org/artist/c5a0a216-c33c-42dc-92b8-4b60236f8dad"},"name":"Con te partirò","creditedTo":"Andrea Bocelli","@id":"https://musicbrainz.org/release-grou