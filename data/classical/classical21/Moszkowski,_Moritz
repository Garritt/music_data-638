aux","byArtist":[{"name":"Сергей Рахманинов","@id":"https://musicbrainz.org/artist/44b16e44-da77-4580-b851-0d765904573e","@type":["Person","MusicGroup"]},{"@id":"https://musicbrainz.org/artist/d7b6ee3a-1035-4895-9f12-91040301dadd","name":"Moritz Moszkowski","@type":["Person","MusicGroup"]},{"@type":["Person","MusicGroup"],"name":"Elizabeth Wolff","@id":"https://musicbrainz.org/artist/f5f8dd41-780f-4116-b89d-22d221875b8e"}],"creditedTo":"Sergei Rachmaninoff, Moritz Moszkowski; Elizabeth Wolff","@type":"MusicAlbum"},{"byArtist":[{"@id":"https://musicbrainz.org/artist/d7b6ee3a-1035-4895-9f12-91040301dadd","name":"Moritz Moszkowski","@type":["Person","MusicGroup"]},{"name":"Mieczysław Karłowicz","@id":"https://musicbrainz.org/artist/bc9dceeb-47cf-436d-b557-b2220820e175","@type":["Person","MusicGroup"]},{"name":"Tasmin Little","@id":"https://musicbrainz.org/artist/e24be060-b525-4502-bd5b-e3ae8ab39c63","@type":["Person","MusicGroup"]},{"@type":"MusicGroup","@id":"https://musicbrainz.org/artist/7edb9f26-0068-4319-8fee-ab70ec2ee37b","name":"BBC Scottish Symphony Orchestra"},{"@type":["Person","MusicGroup"],"@id":"https://musicbrainz.org/artist/6a2b7c54-e605-42da-ae82-e605bce614f1","name":"Martyn Brabbins"}],"albumProductionType":"http://schema.org/StudioAlbum","@id":"https://musicbrainz.org/release-group/25547edf-b376-4ca6-a60b-30e406a9f40b","albumReleaseType":"http://schema.org/AlbumRelease","name":"The Romantic Violin Concerto, Volume 4: Moszkowski: Violin Concerto in C, op. 30 / Ballade in G minor, op. 16 no. 1 / Karłowicz: Violin Concerto in A, op. 8","@type":"MusicAlbum","creditedTo":"Moszkowski, Karłowicz; Tasmin Little, BBC Scottish Symphony Orchestra, Martyn Brabbins"},{"@type":"MusicAlbum","creditedTo":"Rossi, Barber, Scriabin, Lauren Bernofsky, Moszkowski; Jón Sigurðsson","byArtist":[{"@type":["Person","MusicGroup"],"name":"Wynn-Anne Rossi","@id":"https://musicbrainz.org/artist/c98bda97-5ef1-4329-98ce-bbe910e27862"},{"name":"Samuel Barber","@id":"https://musicbrai